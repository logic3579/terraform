# GCP Terraform Variables Example
# Rename this file to terraform.tfvars and update with your values

project_id = "your-gcp-project-id"
region     = "us-central1"
zone       = "us-central1-a"

# Optional overrides
# vpc_name            = "custom-vpc"
# subnet_name         = "custom-subnet"
# subnet_cidr         = "10.0.0.0/24"
# vm_name             = "custom-instance"
# vm_machine_type     = "e2-medium"
# vm_image_family     = "debian-11"
# vm_image_project    = "debian-cloud"